{
    "name": "Instructure.com Content Checker",
    "version": "1.0",
    "description": "Adds checkboxes to the instructure.com content links for tracking.",
    "permissions": ["storage"],
    "content_scripts": [
      {
        "matches": ["https://*.instructure.com/courses/*"],
        "js": ["main.js"]
      }
    ],
    "icons": { 
      "16": "assets/icon16.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
    },    
    "manifest_version": 2
  }